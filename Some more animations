#Makes it go in different directions

recx = recx + velx
    recy = recy + vely
    # ToDo draw rectangle
    if recx < 0:
        velx = 1
        glColor3f(1,0,0)
    if recy < 0:
        vely = 4
        glColor3f(0,1,0)
    if recx > 400:
        velx = -1
        glColor3f(1,0,1)
    if recy > 400:
        vely = -4
        glColor3f(1,1,0)